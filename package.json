  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern /public --ignore-pattern /static --ignore-pattern /node_modules --ignore-pattern /cypress .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "deploy": "gatsby build --prefix-paths && s3-deploy './public/**' --cwd './public/' --bucket benenewton.com --deleteRemoved --gzip && npm run clear",
    "clear": "aws-cloudfront-invalidate E2EGPLKUE6UEOR",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record --key 501a8584-9e09-4347-8deb-ed66f95cd5bf"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
